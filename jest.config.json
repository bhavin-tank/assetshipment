{
 "setupFiles": ["<rootDir>/src/home.test.js"],
 "testRegex": "/*.test.js$",
 "collectCoverage": true,
 "coverageReporters": ["lcov"],
 "coverageDirectory": "test-coverage",
 "coverageThreshold": {
  "global": {
  "branches": 0,
  "functions": 0,
  "lines": 0,
  "statements": 0
  }
 },
 "watchPathIgnorePatterns": ["<rootDir>/node_modules/", "<rootDir>/.git/"],
 "moduleDirectories": ["node_modules", "src"]
}
